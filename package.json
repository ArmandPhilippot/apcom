{
  "name": "apcom",
  "description": "The frontend of ArmandPhilippot.com built with Next JS.",
  "version": "1.1.0",
  "license": "MIT",
  "author": {
    "name": "Armand Philippot",
    "email": "contact@armandphilippot.com",
    "url": "https://www.armandphilippot.com/"
  },
  "homepage": "https://github.com/ArmandPhilippot/apcom#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ArmandPhilippot/apcom.git"
  },
  "bugs": {
    "url": "https://github.com/ArmandPhilippot/apcom/issues"
  },
  "private": true,
  "scripts": {
    "predev": "yarn run i18n:compile",
    "dev": "next dev",
    "prebuild": "yarn run i18n:compile",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "deploy": "sh ./bin/deploy.sh",
    "i18n:compile": "formatjs compile-folder src/i18n lang/",
    "i18n:extract": "formatjs extract 'src/**/*.ts*' --out-file src/i18n/en.json",
    "release": "standard-version -s",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "test:unit": "jest",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "@formatjs/swc-plugin": "^1.4.6",
    "@mdx-js/loader": "^2.1.2",
    "@mdx-js/react": "^2.1.2",
    "@next/bundle-analyzer": "^12.2.3",
    "@next/mdx": "^12.2.3",
    "feed": "^4.2.2",
    "graphql": "^16.1.0",
    "modern-normalize": "^1.1.0",
    "next": "^12.2.3",
    "next-sitemap": "^3.1.11",
    "next-themes": "^0.2.0",
    "prismjs": "^1.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.0.5",
    "schema-dts": "^1.1.0",
    "sharp": "^0.30.7",
    "swr": "^1.3.0",
    "use-ackee": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@formatjs/cli": "^5.0.6",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.6",
    "@types/mdx": "^2.0.1",
    "@types/node": "^18.0.6",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.15",
    "@types/use-ackee": "^2.0.0",
    "cypress": "^10.3.1",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^4.0.2",
    "eslint-plugin-storybook": "^0.6.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "next-router-mock": "^0.7.4",
    "postcss": "^8.4.12",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "standard-version": "^9.3.2",
    "storybook-addon-next": "^1.6.7",
    "storybook-dark-mode": "^1.1.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0"
  }
}
